{
  "name": "boutique-ecommerce",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:pwa": "vite build && npm run generate-sw",
    "generate-sw": "workbox generateSW workbox-config.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "update-products": "node scripts/convertSheetToJson.js",
    "update-products:custom": "node scripts/convertSheetToJson.js",
    "watch-products": "node scripts/watchAndUpdate.js",
    "dev-auto": "node scripts/devWithAutoUpdate.js",
    "optimize-media": "node scripts/optimize-media.js",
    "build:optimized": "npm run optimize-media && vite build",
    "analyze": "vite build --mode analyze",
    "lighthouse": "lighthouse http://localhost:4173 --output html --output-path ./lighthouse-report.html",
    "lighthouse:ci": "lhci autorun --config=lighthouserc.cjs",
    "cache:purge": "echo 'Use: vercel cache rm <url> to purge specific cache entries'",
    "perf:audit": "npm run build && npm run lighthouse:ci"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@vercel/speed-insights": "^1.2.0",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^12.23.9",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-masonry-css": "^1.0.16",
    "react-router-dom": "^7.7.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@lhci/cli": "^0.15.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vite-pwa/assets-generator": "^1.0.0",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "sharp": "^0.34.3",
    "tailwindcss": "^3.4.3",
    "vite": "^7.0.4",
    "vite-imagetools": "^7.1.0",
    "vite-plugin-pwa": "^1.0.2"
  },
  "description": "This template provides a minimal setup to get React working in Vite with HMR and some ESLint rules.",
  "main": "eslint.config.js",
  "keywords": [],
  "author": "",
  "license": "ISC"
}
